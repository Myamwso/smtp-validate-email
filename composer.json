{
    "name": "myamwso/smtp-validate-email",
	"description": "Perform email address verification via SMTP",
	"keywords": [
      "smtp",
      "validator",
      "verify",
      "email"
    ],
	"type": "library",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "huangjie",
            "email": "huangjie@ymify.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
      "dealerdirect/phpcodesniffer-composer-installer": "^0.4.2",
      "phpunit/phpunit": "~4.8|~5.1",
      "squizlabs/php_codesniffer": "^3.0",
      "wimg/php-compatibility": "^8.0"
    },
    "autoload": {
      "psr-4": {
        "SMTPValidateEmail\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "SMTPValidateEmail\\Tests\\": "tests"
      }
    },
	"minimum-stability": "dev"
}
